{
  "dependencies": {
    "@floating-ui/react": "^0.26.1",
    "@hookform/resolvers": "^3.1.1",
    "@material-design-icons/svg": "^0.14.10",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^4.29.19",
    "@zodios/core": "^10.9.2",
    "@zodios/react": "^10.4.5",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.12.17",
    "jotai": "^2.2.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-router-dom": "^6.10.0",
    "svgo": "^3.0.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-svgr": "^3.2.0",
    "web-vitals": "^3.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.1",
    "@axe-core/react": "^4.8.1",
    "@faker-js/faker": "^8.0.2",
    "@swc-jotai/debug-label": "^0.0.10",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.2",
    "postcss": "^8.4.31",
    "prettier": "^2.8.7",
    "shadcn-ui": "^0.4.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  },
  "eslintConfig": {
    "overrides": [
      {
        "extends": [
          "plugin:jsonc/recommended-with-json"
        ],
        "files": [
          "*.json"
        ],
        "rules": {
          "jsonc/comma-style": "error",
          "jsonc/sort-keys": "error"
        }
      },
      {
        "extends": [
          "airbnb",
          "airbnb/hooks",
          "airbnb-typescript",
          "eslint:recommended",
          "plugin:@typescript-eslint/recommended",
          "plugin:@tanstack/eslint-plugin-query/recommended",
          "plugin:react/jsx-runtime",
          "plugin:perfectionist/recommended-natural",
          "prettier"
        ],
        "files": [
          "*.ts",
          "*.tsx",
          "*.js",
          "*.jsx"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
          "project": "./tsconfig.eslint.json"
        },
        "plugins": [
          "@tanstack/query",
          "perfectionist",
          "prettier"
        ],
        "rules": {
          "@typescript-eslint/consistent-type-imports": [
            "error",
            {
              "disallowTypeAnnotations": false,
              "fixStyle": "separate-type-imports",
              "prefer": "type-imports"
            }
          ],
          "@typescript-eslint/no-unused-vars": [
            "error",
            {
              "argsIgnorePattern": "^_",
              "destructuredArrayIgnorePattern": "^_",
              "varsIgnorePattern": "^_"
            }
          ],
          "import/extensions": "off",
          "import/no-extraneous-dependencies": "off",
          "import/order": "off",
          "no-console": [
            "error",
            {
              "allow": [
                "warn",
                "error"
              ]
            }
          ],
          "no-param-reassign": "off",
          "no-unused-vars": "off",
          "perfectionist/sort-imports": [
            "error",
            {
              "groups": [
                "side-effect",
                [
                  "type",
                  "builtin-type",
                  "external-type"
                ],
                "internal-type",
                "parent-type",
                "sibling-type",
                [
                  "builtin",
                  "external"
                ],
                "internal",
                "parent",
                "sibling",
                "unknown"
              ],
              "read-tsconfig": true
            }
          ],
          "prettier/prettier": "error",
          "react/jsx-props-no-spreading": "off",
          "react/jsx-sort-props": "off",
          "react/prop-types": "off",
          "react/require-default-props": "off",
          "react/sort-prop-types": "off",
          "sort-imports": "off",
          "sort-keys": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{json,js,jsx}": [
      "eslint"
    ],
    "*.{ts,tsx,json,html,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint",
      "vitest related --run --no-ui --coverage --passWithNoTests"
    ]
  },
  "name": "@hokkyss/vite-react-ts",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "generate": "shadcn-ui add",
    "lint": "eslint .",
    "postinstall": "husky install",
    "prepare": "husky install",
    "preview": "vite preview",
    "snapshot": "vitest run -u --no-ui",
    "test": "vitest --ui --coverage"
  },
  "type": "module",
  "version": "0.0.1"
}
